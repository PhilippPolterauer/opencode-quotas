    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@ai-sdk/provider": "^3.0.2",
    "@opencode-ai/plugin": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "js-yaml": "^4.1.1",
    "typescript": "^5.9.3",
    "yaml": "^2.8.2"
  }
