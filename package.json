{
  "name": "opencode-quotas",
  "version": "0.0.1-beta",
  "description": "Opencode quota hub plugin for Antigravity and Codex",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "packageManager": "bun@1.1.20",
  "bin": {
